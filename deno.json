{
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run --unstable -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.1.3/",
    "std/": "https://deno.land/std@0.221.0/",
    "preact": "npm:preact",
    "@/": "./src/_components/"
  },
  "compilerOptions": {
    "types": [ "lume/types.ts" ],
    "jsx": "react-jsx",
    "jsxImportSource": "npm:preact"
  }
}
